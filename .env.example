# Catalyst Center MCP Server Configuration
# Copy this file to .env and fill in your actual values

# Catalyst Center URL (include https://)
# Example: https://172.31.96.6 or https://catalyst-center.example.com
CATALYST_CENTER_URL=https://your-catalyst-center.example.com

# Authentication credentials
CATALYST_CENTER_USERNAME=your_username
CATALYST_CENTER_PASSWORD=your_password

# SSL Certificate Verification
# Set to "true" for production with valid certificates
# Set to "false" for development or self-signed certificates (e.g., "false")
CATALYST_CENTER_VERIFY_SSL=true
